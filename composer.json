{
    "name": "audriga/jmap-openxport",
    "type": "library",
    "description": "Provides JMAP support to existing PHP-based systems, in the context of the OpenXPort project",
    "keywords": ["data portability", "jmap", "migration", "groupware"],
    "homepage": "https://github.com/audriga/openxport-jmap",
    "license": "MIT",
    "authors": [
        {
            "name": "Stanimir Bozhilov",
            "email": "stanimir@audriga.com"
        },
        {
            "name": "Joris Baum",
            "email": "joris@audriga.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "psr/log": ">=1"
    },
    "require-dev": {
        "phpunit/phpunit": ">=9",
        "squizlabs/php_codesniffer": ">=3"
    },
    "autoload": {
        "classmap": ["src/"]
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    }
}
